<!DOCTYPE html>
<head>
  <title>Effugio</title>
<link rel="stylesheet" type="text/css" href="//netdna.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css">
</head>
<hr>
<div class="container bootstrap snippet">
    <div class="row">
        <div class="col-lg-12">
            <div class="main-box no-header clearfix">
                <div class="main-box-body clearfix">
                   <div class="table-responsive">
                        <table id="ConnectionsTable" class="table user-list" >
                            <thead>
                                <tr>
                                <th><span>User</span></th>
                                <th><span>Age</span></th>
                                
                                </tr>
                            </thead>
                            <tbody id="tbody">
                                {{#each users }}
                                <tr>
                                    <td>
                                        {{!-- <img src="https://bootdey.com/img/Content/user_2.jpg" alt=""> --}}
                                        <img src="http://res.cloudinary.com/effugio/image/upload/c_fill,r_20/{{ _id}}.png" class="avatar img-circle" alt="avatar">

                                        <a href="/users/checkprofile/{{_id}}" class="user-link">{{name}}</a>
                                        <span class="user-subhead">{{location}}</span>
                                    </td>
                                <td class="text-center">
                                        {{{toage dob}}}</td>
                                
                                {{/each}}
                            
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>






